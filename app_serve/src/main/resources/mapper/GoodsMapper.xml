<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="edu.mapper.GoodsMapper">
    <select id="selectDto" resultType="edu.dto.GoodsDto">
        select g.*, u.username, u.name nickname
        from goods g
                 left join user u on u.id = g.user_id
            ${ew.customSqlSegment}
    </select>
</mapper>
