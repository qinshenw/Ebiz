<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="edu.mapper.OrdersMapper">
    <select id="selectDto" resultType="edu.dto.OrdersDto">
        select s.*, u.username, u.name nickname
        from orders s
                 left join user u on u.id = s.user_id
            ${ew.customSqlSegment}
    </select>
</mapper>
